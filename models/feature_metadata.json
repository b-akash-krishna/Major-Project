{
  "primary_features": [
    {
      "name": "anchor_age",
      "label": "Patient Age",
      "type": "number",
      "min": 18,
      "max": 120,
      "required": true
    },
    {
      "name": "los_days",
      "label": "Length of Stay (days)",
      "type": "number",
      "min": 0,
      "max": 100,
      "required": true
    },
    {
      "name": "prev_admissions",
      "label": "Previous Admissions",
      "type": "number",
      "min": 0,
      "max": 20,
      "required": true
    },
    {
      "name": "days_since_last",
      "label": "Days Since Last Admission",
      "type": "number",
      "min": 0,
      "max": 999,
      "default": 999
    },
    {
      "name": "admission_type",
      "label": "Admission Type",
      "type": "select",
      "options": [
        {
          "value": 1,
          "label": "Emergency"
        },
        {
          "value": 2,
          "label": "Urgent"
        },
        {
          "value": 3,
          "label": "Elective"
        }
      ]
    },
    {
      "name": "gender",
      "label": "Gender",
      "type": "select",
      "options": [
        {
          "value": 0,
          "label": "Female"
        },
        {
          "value": 1,
          "label": "Male"
        }
      ]
    },
    {
      "name": "had_icu",
      "label": "ICU Stay",
      "type": "boolean"
    },
    {
      "name": "icu_los_sum",
      "label": "ICU Hours (if applicable)",
      "type": "number",
      "min": 0,
      "max": 720,
      "default": 0
    },
    {
      "name": "proc_count",
      "label": "Number of Procedures",
      "type": "number",
      "min": 0,
      "max": 20,
      "default": 0
    },
    {
      "name": "dx_count",
      "label": "Number of Diagnoses",
      "type": "number",
      "min": 0,
      "max": 50,
      "default": 1
    },
    {
      "name": "prev_readmit_rate",
      "label": "Previous Readmission Rate",
      "type": "number",
      "min": 0,
      "max": 1,
      "step": 0.1,
      "default": 0
    },
    {
      "name": "ed_time_hours",
      "label": "ED Wait Time (hours)",
      "type": "number",
      "min": 0,
      "max": 48,
      "default": 0
    },
    {
      "name": "transfer_count",
      "label": "Number of Transfers",
      "type": "number",
      "min": 0,
      "max": 10,
      "default": 0
    },
    {
      "name": "discharge_location",
      "label": "Discharge Location",
      "type": "select",
      "options": [
        {
          "value": 6,
          "label": "Home"
        },
        {
          "value": 7,
          "label": "Home Health Care"
        },
        {
          "value": 11,
          "label": "Rehab"
        },
        {
          "value": 12,
          "label": "Skilled Nursing Facility"
        }
      ]
    }
  ],
  "auto_calculated": [
    {
      "name": "los_hours",
      "formula": "los_days * 24"
    },
    {
      "name": "age_los",
      "formula": "anchor_age * los_days"
    },
    {
      "name": "proc_per_day",
      "formula": "proc_count / (los_days + 1)"
    },
    {
      "name": "dx_per_day",
      "formula": "dx_count / (los_days + 1)"
    },
    {
      "name": "icu_los_ratio",
      "formula": "icu_los_sum / (los_hours + 1)"
    },
    {
      "name": "severity_score",
      "formula": "calculated from multiple inputs"
    },
    {
      "name": "complexity_score",
      "formula": "calculated from multiple inputs"
    }
  ],
